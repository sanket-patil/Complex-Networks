% Produce the title:
%    \begin{macrocode}
  \frontmatter@title@produce
%    \end{macrocode}
% Produce the author list:
%    \begin{macrocode}
  \groupauthors@sw{%
   \frontmatter@author@produce@group
  }{%
   \frontmatter@author@produce@script
  }%
