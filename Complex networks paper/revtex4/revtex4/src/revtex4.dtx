%
% Note: this procedure expects \cmd\@tempcnta\ to reflect the current number of items remaining to list off.
%    \begin{macrocode}
\def\@listcomma@comma{\@ifnum{\@tempcnta>\@ne}{,}{}}%
\def\@listcomma@comma@UK{\@ifnum{\@tempcnta>\tw@}{,}{}}%
